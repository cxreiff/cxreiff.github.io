{
  "name": "jaxreiff-site",
  "version": "1.0.0",
  "description": "Portfolio site for Jax Reiff.",
  "author": "Jax Reiff <jaxreiff@me.com> (https://jaxreiff.com)",
  "homepage": "https://jaxreiff.com/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaxreiff/jaxreiff.github.io.git"
  },
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "analyze": "npm run build -- --reporter @parcel/reporter-bundle-analyzer",
    "tsm": "typed-scss-modules src -n none --L error -o tsm",
    "tsc": "npm run tsm && tsc --noEmit",
    "watch": "npm run tsm -- --watch & tsc -w",
    "lint": "eslint --cache --ext .ts,.tsx .",
    "jest": "jest",
    "test": "npm run tsc && npm run lint && npm run jest",
    "cname": "cp CNAME dist/",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prepare": "husky install"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime"
  },
  "dependencies": {
    "@babylonjs/core": "^4.2.2",
    "@babylonjs/gui": "^4.2.0",
    "@reduxjs/toolkit": "^1.8.0",
    "assets": "^3.0.1",
    "babylonjs-hook": "^0.1.0",
    "buffer": "^6.0.3",
    "classnames": "^2.3.1",
    "follow-redirects": "^1.14.9",
    "gray-matter": "^4.0.3",
    "history": "^4.10.1",
    "normalize.css": "^8.0.1",
    "pixi.js": "^6.3.0",
    "preact": "^10.6.6",
    "preact-router": "^4.0.1",
    "react-infinite-scroll-hook": "^4.0.1",
    "react-markdown": "^8.0.1",
    "react-modal-image": "^2.5.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^6.2.2",
    "react-syntax-highlighter": "^15.5.0",
    "redux": "^4.1.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@jest/types": "^27.1.1",
    "@parcel/babel-preset-env": "^2.4.0",
    "@parcel/reporter-bundle-analyzer": "^2.4.0",
    "@parcel/transformer-glsl": "^2.4.0",
    "@parcel/transformer-sass": "^2.4.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^27.4.1",
    "@types/jsdom": "^16.2.3",
    "@types/react": "^17.0.43",
    "@types/react-router": "^5.1.14",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/redux-logger": "^3.0.8",
    "@types/redux-mock-store": "^1.0.3",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "babel-jest": "^27.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^3.4.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.11.0",
    "eslint-config-preact": "^1.1.4",
    "eslint-plugin-jest": "^26.1.3",
    "gh-pages": "^3.1.0",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.0",
    "jest-transform-stub": "^2.0.0",
    "json-schema": ">=0.4.0",
    "lint-staged": "^12.3.7",
    "marked": "^4.0.10",
    "minimist": ">=1.2.6",
    "nanoid": "^3.3.2",
    "node-forge": ">=1.3.0",
    "parcel": "^2.4.0",
    "postcss": ">=8.4.12",
    "postcss-modules": "^4.3.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "react-router": "^5.2.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.49.9",
    "stream-browserify": "^3.0.0",
    "timers-browserify": "^2.0.12",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typed-scss-modules": "^6.2.0",
    "typescript": "^4.6.2"
  }
}
