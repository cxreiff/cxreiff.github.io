{
  "name": "cxreiff.github.io",
  "version": "1.0.0",
  "description": "Portfolio site for Cooper Reiff.",
  "author": "Cooper Reiff <dev@cooperreiff.com> (https://cooperreiff.com)",
  "homepage": "https://cooperreiff.com/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cxreiff/cxreiff.github.io.git"
  },
  "source": "src/index.html",
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "analyze": "pnpm run build -- --reporter @parcel/reporter-bundle-analyzer",
    "tsm": "typed-scss-modules src -n none --L error -o tsm",
    "tsc": "pnpm run tsm && tsc --noEmit",
    "lint": "eslint --cache .",
    "prettier": "prettier --write src",
    "test": "vitest run",
    "checks": "pnpm run tsc && pnpm run lint && pnpm run prettier && pnpm run test",
    "cname": "cp CNAME dist/",
    "tsc:husky": "pnpm run tsm && tsc-files --noEmit",
    "lint:husky": "eslint",
    "prettier:husky": "prettier --write",
    "test:husky": "vitest related --run",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "pnpm run tsc:husky",
      "pnpm run lint:husky",
      "pnpm run prettier:husky",
      "pnpm run test:husky"
    ],
    "*.{html,css,scss,js,json,md}": [
      "pnpm run prettier:husky"
    ]
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime",
    "~*": "./*"
  },
  "devDependencies": {
    "@parcel/core": "^2.8.3",
    "@parcel/reporter-bundle-analyzer": "^2.8.3",
    "@parcel/transformer-glsl": "^2.8.3",
    "@parcel/transformer-sass": "^2.8.3",
    "@preact/preset-vite": "^2.5.0",
    "@reduxjs/toolkit": "^1.9.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/preact": "^3.2.3",
    "@types/jsdom": "^21.1.0",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^17.0.19",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/redux-logger": "^3.0.9",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "assets": "^3.0.1",
    "buffer": "^5.5.0",
    "classnames": "^2.3.2",
    "eslint": "^8.34.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "events": "^3.3.0",
    "gray-matter": "^4.0.3",
    "happy-dom": "^8.2.6",
    "husky": "^8.0.3",
    "json-schema": ">=0.4.0",
    "lint-staged": "^13.1.1",
    "lucide-preact": "^0.127.0",
    "marked": "^4.2.12",
    "normalize.css": "^8.0.1",
    "parcel": "^2.8.3",
    "postcss": "^8.4.21",
    "postcss-modules": "^4.3.0",
    "preact": "^10.12.1",
    "prettier": "^2.8.4",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "react": "npm:@preact/compat@^17.1.2",
    "react-dom": "npm:@preact/compat@^17.1.2",
    "react-infinite-scroll-hook": "^4.0.4",
    "react-markdown": "^8.0.5",
    "react-modal-image": "^2.6.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.1",
    "react-syntax-highlighter": "^15.5.0",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "stream": "^0.0.2",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0",
    "timers-browserify": "^2.0.12",
    "tsc-files": "^1.1.3",
    "typed-scss-modules": "^7.0.2",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5",
    "vitest-fetch-mock": "^0.2.2",
    "xml2js": "^0.4.23"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/core",
        "vite"
      ]
    }
  }
}
